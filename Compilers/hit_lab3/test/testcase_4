int main()
{
	int a[20], i = 0, j = 0, max = 0, min = 1000000, min_index = 0, max_index = 0;
	
	while (i < 20)
	{
		a[i] = j * -1 + j * j / 100 - 19;
		j = a[i];
		i = i + 1;
	}
	
	i = 0;
	while (i < 20) 
	{
		if (a[i] < min)
		{
			min = a[i];
			min_index = i;
		}
		if (a[i] > max)
		{
			max = a[i];
			max_index = i;
		}
		i = i + 1;
	}
	
	write(min_index);write(min);
	write(max_index);write(max);
	return 0;
}
